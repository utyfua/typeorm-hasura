{
  "name": "typeorm-hasura",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "typeorm": "typeorm-ts-node-esm",
    "test:gen": "npx env-cmd npx typeorm-ts-node-esm migration:generate ./test/migration/next -d ./test/data-source",
    "test:up": "npx env-cmd npx typeorm-ts-node-esm  migration:run -d ./test/data-source",
    "test:down": "npx env-cmd npx typeorm-ts-node-esm  migration:revert -d ./test/data-source",
    "test:start": "npx env-cmd ts-node test/index.ts",
    "test:run": "npx env-cmd ts-node test/hasura.ts"
  },
